<odoo>
    <data noupdate="1">
        <!-- Project Stages (Task Types) -->
        <record id="project_stage_concept" model="project.task.type">
            <field name="name">التصميم المبدئي (الكروكي)</field>
            <field name="sequence">10</field>
        </record>
        <record id="project_stage_contract" model="project.task.type">
            <field name="name">التعاقد وجمع الوثائق</field>
            <field name="sequence">20</field>
        </record>
        <record id="project_stage_external" model="project.task.type">
            <field name="name">الموافقات الخارجية</field>
            <field name="sequence">30</field>
        </record>
        <record id="project_stage_detailed" model="project.task.type">
            <field name="name">التصميمات التفصيلية</field>
            <field name="sequence">40</field>
        </record>
        <record id="project_stage_supervision" model="project.task.type">
            <field name="name">الإشراف الهندسي</field>
            <field name="sequence">50</field>
        </record>
        <record id="project_stage_completion" model="project.task.type">
            <field name="name">إنهاء المشروع</field>
            <field name="sequence">60</field>
        </record>
    </data>
</odoo>